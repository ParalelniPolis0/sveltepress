{
  "name": "@sveltepress/theme-default",
  "homepage": "https://sveltepress.site/",
  "version": "0.14.1",
  "description": "The default theme for sveltepress",
  "scripts": {
    "test": "vitest",
    "cp:assets": "cp src/style.css dist && cp -r src/fonts dist",
    "build:comps": "svelte-package && rm dist/package.json",
    "build": "pnpm build:comps && pnpm cp:assets"
  },
  "files": [
    "dist",
    "types.d.ts"
  ],
  "keywords": [
    "Docs",
    "svelte",
    "svelte docs",
    "sveltepress"
  ],
  "author": {
    "name": "Dongsheng Zhao",
    "email": "1197160272@qq.com"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/Blackman99/sveltepress",
    "directory": "packages/theme-default"
  },
  "license": "MIT",
  "devDependencies": {
    "@sveltepress/vite": "workspace:*",
    "@sveltejs/kit": "^1.0.1",
    "@sveltejs/package": "^1.0.1",
    "mdsvex": "^0.10.6",
    "svelte": "^3.55.0",
    "typescript": "^4.9.4",
    "unified": "^10.1.2",
    "vite": "^4.0.3",
    "vitest": "^0.26.3"
  },
  "dependencies": {
    "@docsearch/css": "3",
    "@docsearch/js": "3",
    "lru-cache": "^7.14.1",
    "remark-admonitions": "^1.2.1",
    "shiki": "^0.12.1",
    "uid": "^2.0.1",
    "unist-util-visit": "^4.1.1",
    "unocss": "^0.48.0"
  },
  "type": "module",
  "types": "./src/index.ts",
  "exports": {
    ".": "./dist/index.js",
    "./style.css": "./src/style.css",
    "./CExpansion.svelte": "./src/components/CExpansion.svelte",
    "./PageLayout.svelte": "./src/components/PageLayout.svelte",
    "./GlobalLayout.svelte": "./src/components/GlobalLayout.svelte",
    "./Link.svelte": "./src/components/Link.svelte",
    "./CopyCode.svelte": "./src/components/CopyCode.svelte",
    "./package.json": "./package.json",
    "./types": {
      "types": "./types.d.ts"
    }
  },
  "publishConfig": {
    "types": "./dist/index.d.ts",
    "exports": {
      ".": "./dist/index.js",
      "./style.css": "./dist/style.css",
      "./CExpansion.svelte": "./dist/components/CExpansion.svelte",
      "./PageLayout.svelte": "./dist/components/PageLayout.svelte",
      "./Link.svelte": "./dist/components/Link.svelte",
      "./GlobalLayout.svelte": "./dist/components/GlobalLayout.svelte",
      "./CopyCode.svelte": "./dist/components/CopyCode.svelte",
      "./package.json": "./package.json",
      "./types": {
        "types": "./types.d.ts"
      }
    }
  }
}
